{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb/base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-alert":"off",
        "no-plusplus":"off",
        "no-console":"off",
        "max-len":["error", 200],
        "no-param-reassign": "off",
        "no-restricted-syntax": "off"
    }
}
